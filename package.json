    "node-sass": "^4.9.2",
    "postcss-loader": "^1.2.2",
    "resolve-url-loader": "^2.1.1",
    "rework-plugin-url": "^1.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
