{
  "name": "react-cm-ui",
  "version": "1.13.7-hotfix",
  "description": "React UI for Church management",
  "main": "lib/",
  "jsnext:main": "src/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saddlebackdev/react-cm-ui.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/saddlebackdev/react-cm-ui/issues"
  },
  "keywords": [
    "button",
    "drawer",
    "grid",
    "icon",
    "label",
    "responsive",
    "react",
    "react-component",
    "church management",
    "cm"
  ],
  "homepage": "https://github.com/saddlebackdev/react-cm-ui#readme",
  "scripts": {
    "build": "webpack",
    "build:local": "webpack-dev-server --content-base docs/build/ --hot --progress --colors --host 0.0.0.0",
    "build:prod": "npm run build",
    "build:stats": "webpack --display-modules --display-chunks --json > stats.json --progress",
    "clean:lib": "rimraf lib",
    "compile": "npm run clean:lib && gulp compile:lib && cpx \"src/fonts/**\" \"lib/fonts\" && cpx \"src/images/**\" \"lib/images\" && babel src/ -d lib/",
    "postpublish": "git checkout master && git push && git checkout dev",
    "prepare": "npm run compile",
    "start": "npm run build:local"
  },
  "dependencies": {
    "autosize": "^3.0.17",
    "classnames": "^2.2.5",
    "country-telephone-data": "^0.4.4",
    "highcharts": "^6.1.1",
    "immutability-helper": "^2.7.1",
    "lodash": "^4.15.0",
    "moment-timezone": "^0.5.21",
    "react": "^15.6.2",
    "react-custom-scrollbars": "^4.0.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^15.6.2",
    "react-onclickoutside": "^5.7.1",
    "react-portal": "^3.0.0",
    "react-responsive": "^1.2.7",
    "react-router": "^3.0.0",
    "react-select": "^1.3.0",
    "react-syntax-highlighter": "^4.0.1",
    "react-tether": "^0.5.7",
    "react-text-mask": "5.4.1",
    "react-tree-menu": "^1.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.10.0",
    "flag-icon-css": "^2.8.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-rework": "^1.2.0",
    "gulp-sass": "^3.1.0",
    "hoek": "^4.2.1",
